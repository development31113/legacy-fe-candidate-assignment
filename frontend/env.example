# Dynamic.xyz Configuration
# Get your environment ID from https://app.dynamic.xyz/
NEXT_PUBLIC_DYNAMIC_ENVIRONMENT_ID=your_dynamic_environment_id

# Vercel KV Configuration (for production)
# These will be set automatically by Vercel when you connect KV
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Backend URL (fallback for local development)
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# Vercel URL (for production)
NEXT_PUBLIC_VERCEL_URL=

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Development Settings
NODE_ENV=development 